{
    "name": "svelte-number-displayer",
    "version": "1.0.2",
    "description": "Svelte number displayer",
    "author": "n4n5 - n4n5.dev",
    "homepage": "https://github.com/Its-Just-Nans/svelte-number-displayer",
    "bugs": "https://github.com/Its-Just-Nans/svelte-number-displayer/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/Its-Just-Nans/svelte-number-displayer.git"
    },
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "svelte": "dist/index.js",
    "types": "src/index.ts",
    "keywords": [
        "svelte",
        "number",
        "fade",
        "animation",
        "svelte-components",
        "sveltejs"
    ],
    "scripts": {
        "build": "npm run build:js && npm run build:svelte",
        "build:js": "tsc -p . --outDir dist/ --rootDir src/",
        "build:svelte": "svelte-strip strip src/ dist",
        "check": "npx svelte-check --ignore dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "license": "MIT",
    "devDependencies": {
        "svelte-check": "^3.1.4",
        "svelte-strip": "^2.0.0",
        "tslib": "^2.5.0",
        "@tsconfig/svelte": "^4.0.1",
        "typescript": "^5.0.3"
    }
}
